pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- animation system
-- 1st jam: utz vs. grinchian
-- 29.11.17
-- 90 % riki klein
-- 10 % christoph kerls
-- v1


function _init()
	frame     = 1
	fps       = 30
	sec       = 0
	
	
	music(0)
	
	state = 1

	
	player = {
		x      = 64,
		y      = 90,
		vx     = 1,
		vy     = 1,
		frames = {
			{96, 97},    -- 1 = left
			{80, 81},    -- 2 = right
			{112, 113},  -- 3 = up
		 {64, 65}		   -- 4 = down
		},
		findex = 1,
		ffreq  = 5,
		astate = 3
	}
	
	enemy = {
		x = 64,
		y = 64,
		frames = {85, 86, 87, 86},
		vx = 2,
		vy = 2,
		findex = 1,
		ffreq  = 5,
		sr = 0         -- spawnrate
	}

cam = {
  x = 64,
  y = 64,
  x_min = 64,
  y_min = 64
 }
	
end


function _update()
 if (state == 0) then
  -- main menu logic
  update_mainmenu()
  
 elseif (state == 1) then
  -- gameplay logic
  update_gameplay()

  
 elseif (state == 2) then
  -- death screen logic
  update_death()
 end
end


function update_mainmenu()
	
	-- press x to start
	if (btnp(5)) then
		state = 1
	end
end

function update_gameplay()

	-- player movement l/r
	if (btn(0))
	and (player.x > 0) then
		player.x -= player.vx
		player.astate = 0
	elseif (btn(1))
	and (player.x + 4 < 255) then
		player.x += player.vx
		player.astate = 1
	end
	
	--player movement u/d
	if (btn(2))
	and (player.y > 0) then
		player.y -= player.vy
		player.astate = 2
	elseif (btn(3))
	and (player.y + 5 < 255) then
		player.y += player.vy
		player.astate = 3
	end		

 -- drop gift
 if (btnp(4)) then
  mset(
   player.x/8, -- map tile x
   player.y/8, -- map tile y
   77
  )
 end

	-- player animation
	if (frame%player.ffreq == 0) then
		player.findex += 1
	
		if (player.findex > #player.frames[1]) then
			player.findex = 1
		end
	end

	-- enemy movement + loss
	for e in all(enemy) do
		e.x += e.vx
		e.y += e.vy
		
		if  (e.x + 6 >= player.x)
		and (e.x <= player.x + 4)
		and (e.y + 7 >= player.y + 2)
		and (e.y <= player.y +7) then
			state = 2
		end
		
	end
	
	
	-- enemy animation
	if (frame%enemy.ffreq == 0) then
		enemy.findex += 1
	
		if (enemy.findex > #enemy.frames) then
			enemy.findex = 1
		end
	end
	
	
 -- add enemy 
 if (enemy.sr > 20) then
  add_enemy(
   	64,
  	-10
  )
  
  enemy.sr = 0
 end
 
 

	frame            += 1
	enemy.sr         += 1
	

	if (frame%fps == 0) then
		sec += 1		
	end
	
 -- camera motion
 cam.x = player.x
 cam.y = player.y
 
 if (cam.x < cam.x_min) then
  cam.x = cam.x_min
 end
 
 if (cam.y < cam.y_min) then
  cam.y = cam.y_min
 end

end


function update_death()
	
	-- restart game
	if (btnp(5))then
		_init()	
	end
end


function add_enemy(_x, _y)
	local e = {
		x  = _x,
		y  = _y,
		vx =  rnd(2) - 1,
		vy =  rnd(2)
 }
 
 add(enemy, e)
end



function _draw()
 cls()
 
 if (state == 0) then
  -- main menu output
  draw_mainmenu()
  
 elseif (state == 1) then
  -- gameplay output
  draw_gameplay()
  
 elseif (state == 2) then
  -- death output
  draw_death()
 end 
end


function draw_mainmenu()
	
	print("press x to save utz", 30, 60, 8)
	print("from the evil krystian...", 20, 67, 8)
	
end


function draw_gameplay()

	cls()
	
	-- map set
	map(0, 0, 0, 0, 32, 32)
	
	-- camera
 camera(cam.x - 64, cam.y - 64)

 --[[
	print("run utz, run!", 40, 1, 8)
	print("watch out for krystian!", 20, 7, 8)
	print(sec.."s survived so far", 30, 122, 8)
 ]]
 
 -- player
	spr(
		player.frames[player.astate+1][player.findex],
		player.x,
		player.y
	)
	
	-- enemy
	for e in all(enemy) do
  spr(
   enemy.frames[enemy.findex],
  	e.x,
  	e.y
  )
 end
 
 
	
end


function draw_death()
	cls()
	
 print("you survived for "..sec.." seconds", 10, 60, 8)
	print("but krystian still won!", 20, 66, 8)
	print("press x to try again", 24, 72, 8)
end
